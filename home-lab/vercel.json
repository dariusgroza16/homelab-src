{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/api/index.py" },
    { "src": "^/api/services$", "dest": "/api/services.py" },
    { "src": "^/static/(.*)$", "dest": "/static/$1" },
    { "src": "^/(.*)$", "dest": "/static/$1" }
  ]
}
